  it<span class="sy1">=</span>set_symmetric_difference <span class="br0">&#40;</span>first, first<span class="sy2">+</span><span class="nu0">5</span>, second, second<span class="sy2">+</span><span class="nu0">5</span>, v.<span class="me1">begin</span><span class="br0">&#40;</span><span class="br0">&#41;</span><span class="br0">&#41;</span><span class="sy4">;</span>
