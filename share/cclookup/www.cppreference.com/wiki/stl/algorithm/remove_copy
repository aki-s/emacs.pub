<div class="level3">
<pre class="cpp code cpp"><span class="kw2">template</span><span class="sy1">&lt;</span><span class="kw2">class</span> InputIterator, <span class="kw2">class</span> OutputIterator, <span class="kw2">class</span> T<span class="sy1">&gt;</span>
OutputIterator remove_copy<span class="br0">&#40;</span>InputIterator first,
                           InputIterator last,
                           OutputIterator d_first,
                           <span class="kw4">const</span> T<span class="sy3">&amp;</span> value<span class="br0">&#41;</span>
<span class="br0">&#123;</span>
    <span class="kw1">for</span> <span class="br0">&#40;</span><span class="sy4">;</span> first <span class="sy3">!</span><span class="sy1">=</span> last<span class="sy4">;</span> <span class="sy2">++</span>first<span class="br0">&#41;</span>
        <span class="kw1">if</span> <span class="br0">&#40;</span><span class="sy3">!</span><span class="br0">&#40;</span><span class="sy2">*</span>first <span class="sy1">==</span> value<span class="br0">&#41;</span><span class="br0">&#41;</span> <span class="br0">&#123;</span>
            <span class="sy2">*</span>d_first<span class="sy2">++</span> <span class="sy1">=</span> <span class="sy2">*</span>first<span class="sy4">;</span>
        <span class="br0">&#125;</span>
    <span class="br0">&#125;</span>
    <span class="kw1">return</span> d_first<span class="sy4">;</span>
<span class="br0">&#125;</span></pre>
</div>
<!-- EDIT4 SECTION "Equivalent function" [710-1149] -->
<h3 class="sectionedit5"><a name="complexity" id="complexity">Complexity</a></h3>
<div class="level3">

<p>

linear in the distance between <code>first</code> and <code>last</code>
</p>

</div>
