<div class="level3">
<pre class="cpp code cpp"><span class="kw2">template</span><span class="sy1">&lt;</span><span class="kw2">class</span> ForwardIterator<span class="sy1">&gt;</span>
<span class="kw4">void</span> rotate<span class="br0">&#40;</span>ForwardIterator first, ForwardIterator n_first, ForwardIterator last<span class="br0">&#41;</span>
<span class="br0">&#123;</span>
    ForwardIterator next <span class="sy1">=</span> n_first<span class="sy4">;</span>
    <span class="kw1">while</span> <span class="br0">&#40;</span>first <span class="sy3">!</span><span class="sy1">=</span> next<span class="br0">&#41;</span> <span class="br0">&#123;</span>
        std<span class="sy4">::</span><span class="me2">swap</span><span class="br0">&#40;</span><span class="sy2">*</span>first<span class="sy2">++</span>, <span class="sy2">*</span>next<span class="sy2">++</span><span class="br0">&#41;</span><span class="sy4">;</span>
        <span class="kw1">if</span> <span class="br0">&#40;</span>next <span class="sy1">==</span> last<span class="br0">&#41;</span> <span class="br0">&#123;</span>
            next <span class="sy1">=</span> n_first<span class="sy4">;</span>
        <span class="br0">&#125;</span> <span class="kw1">else</span> <span class="kw1">if</span> <span class="br0">&#40;</span>first <span class="sy1">==</span> n_first<span class="br0">&#41;</span> <span class="br0">&#123;</span>
